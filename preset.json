{
	"useConfigFiles": true,
	"plugins": {
		"@vue/cli-plugin-babel": {},
		"@vue/cli-plugin-router": {
			"historyMode": true
		},
		"@vue/cli-plugin-vuex": {}
	},
	"cssPreprocessor": "scss",
  "configs" : {
    "eslintConfig": {
      "root": true,
      "env": {
        "node": true
      },
      "extends": [
        "plugin:vue/essential",
        "eslint:recommended"
      ],
      "parserOptions": {
        "parser": "babel-eslint"
      },
      "rules": {}
    },
    "devServer": {
      "port": 1779,
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  }
}